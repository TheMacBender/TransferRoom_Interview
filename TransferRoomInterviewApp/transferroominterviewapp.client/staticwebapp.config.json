{
    "routes": [
      {
        "route": "/api/*",
        "allowedMethods": ["GET", "POST"],
        "rewrite": "/api"
      }
    ],
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": [
        "/static/*",
        "/api",
        "/*.js",
        "/*.css",
        "/*.png",
        "/*.jpg",
        "/*.svg"
      ]
    }
}